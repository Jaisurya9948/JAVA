import java.lang.*;
public class MyClass {
    public static void main(String args[]) {
    int sum=0;
    int[] j= new int[5];
    j[0]=2;
    j[1]=5;
    j[2]=1;
    j[3]=9;
    j[4]=6;
    for(int i=0;i<j.length;i++){
        sum=sum+j[i];
        System.out.println(sum);
        }
    System.out.println("The sum of 5 numbers in an array is :" + sum);
    }
}
