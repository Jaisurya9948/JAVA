import java.io.*;
public class constructor{
    public int i;
    public int j;
    public int z;
    public static void main(String []args){
        constructor cp=new constructor(2,3);
        System.out.println(cp.z);
    }
    public constructor() {
        i=20;
    }
    public constructor(int i, int j){
        z=i+j;
    }
}
